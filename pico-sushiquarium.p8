function _init()
-- play vs menu
 state="menu"
-- enable mouse
--poke(0x5f2d, 1)


--setup 64x64 border
	poke(0x5f2c,3)
	border = 63
 money = 500


--debug
	debug = ""
--time
	fr = 0
	
	
-- coordinates for dots (feed) 
 feed = entity:new({
  img = 2,
  x = 30,
  y = 30,
  speed = 0.5,
  price = 5,
  max_life = 30,
  life = 0,
  
  update = function(self)
		 self.y += self.speed
		 
	 	if (self.x - 3 > border) then
	   --@todo change to stay within border, not bounce
	  end
		end,
		
		
		draw = function(self)
			spr(self.img,self.x,self.y)
		end
 })
 
--fish coordinates
 fish = entity:new({
  img = 1,
  --1 for right, -1 for left
  face = 1,
		speed =0.3,
		dir = 0.0,-- angle cc
		feed_target = {},
		feed_dist = 9999,
		
		update = function(self)
		 for feed in all(feeds) do
		 --1.look for closest feed
    latest_dist =  check_dist(feed,self)
		  if (latest_dist < self.feed_dist) then
		   self.feed_target = feed
		   self.feed_dist = latest_dist
		   printh("hey")
		  end
			end
			--2.check angle to this feed
			if self.feed_target.x != nil then
				local dx = self.feed_target.x-self.x
	   local dy = self.feed_target.y-self.y
	    
	   self.dir = atan2(dx, dy)
				--3.move to this angle
	   self.x += self.speed * cos(self.dir)
	   self.y += self.speed * sin(self.dir)
			end
			--border block
		end,
		
		
		draw = function(self)
			spr(self.img,self.x,self.y)
 	 --circfill(self.x-1,self.y,1,11)
		end,
		
		clear_feed = function(self)
		 self.feed_target = {}
		 self.feed_dist = 9999
		end
 })
 
 
 feeds = {}
 fishes = {}
 --rn only color diff, but it should have other meanings
 fish_types = {yellow, green}
 for i = 1,5 do
  add(fishes, fish:new({
   x=flr(4 + rnd(border - 16)),
   y=flr(16 + rnd(border - 32)),
   img=flr(rnd(2))
  }))
 
 end
 
--shark coordinates


--arrow coordinates for food
 ax = 40
 ay = 8
 adir = 1
 as = 0.5
 amins = 1
 aspeedup = -1
 
--music(0,0,3)
end

-->8
--updates and draw for game

function update_game()
	fr += 1
 mousex = stat(32)
 mousey = stat(33)
 
 --if btn(⬅️) then
 if btn(⬅️) then
  debug = "speed"
 	aspeedup = true
 elseif aspeedup and money >= 5 
 	and not btn(⬅️) then
 	debug = "drop"
 	
 	add(feeds, feed:new({
   x=ax,
   y=ay + 1
  }))
  
  money -= feed.price
 	
 	as = amins
 	aspeedup = false
 end
 
 if fr >= 2520 then
 	fr = 0
 end
 
 --frame movement	
 --feed move
 for fish in all(fishes) do
	 fish:update()
 end
 for feed in all(feeds) do
  feed:update()
  if (feed.y - 3 > border) then
		  del(feeds, feed)
		  for fish in all(fishes) do
		   fish:clear_feed()
		  end
		 end
 end
 
 if (fr % 2 == 0) then
  --fish move
  
	elseif (fr % 3 == 0) then
  if aspeedup then
   as += 1
   --debug=as
		end
		
		--arrow border
		ax += as * adir
		if ax > border - 7 then
		 ax = border-7
			adir = -1
		elseif ax - 1 < 0 then
   ax = 0
		 adir = 1
		end
	end
 --end frame movement

end


function draw_game()
 --pal(0,129, 1)
 cls()
 --rectfill(0,0,border,border,0)
 --pal(0,0,0)
 --units
	--feed
	--print("●",dx,dy,8)
	for feed in all(feeds) do
	 feed:draw()
	end
	--shark
	spr(17,30,54,2,1)
	--fish
	
	--sspr(8,0,8,8,f1x,f1y,16,16)
	--spr(1,f1x,f1y,1,1,true,false)	
 for fish in all(fishes) do
	 fish:draw()
 end
--draw border
	rect(0,9,border,border,12)
	rectfill(0,0,border,8,1)
	line(1,62,62,62,4)
--arrow
	spr(4, ax, ay-3)
--ui
 m = tostring(money)
 while #m<4 do
  m = "0"..m
 end
	print("$"..m,44,2,7)

--debug
	print(debug,2, 2)
end

