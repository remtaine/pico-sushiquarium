--updates and draw else

function update_menu()
 if btn(⬅️) then
  state="play"
 end
end

function draw_menu()
	cls()
	--map(0,0,0,0,8,8)
	fc = 7
	print("press ⬅️",15,12,fc)
 print("to start",17,20,fc)
 --todo add instructions
 --print("instructions",8,40,0)
 print("btn - feed fish",4,34,fc)
 print("dont feed shark",4,42,fc)
 print("made by remtaine",0,55,fc)
end

