--backend stuff

--state machine
function _update()
 if state=="menu" then
  update_menu()
 elseif state=="play" then
  update_game()
 end
end

function _draw()
 --[[
 if state=="menu" then
		draw_menu()
	elseif state=="play" then
	]] 
	draw_game()
	--end
end


class = {
 new = function(self,tbl)
	 tbl = tbl or {}
	 setmetatable(tbl, {
	  __index = self
	 })
	 return tbl
	end,
}


entity = class:new({
 x=0,
 y=0,
})

check_dist = function(e1,e2)
 local dx = (e1.x - e2.x)
 local dy = (e1.y - e2.y)

 dx *= dx
 dy *= dy
 return sqrt(dx + dy)
end


check_angle = function(e1,e2)
 local dx = e2.x-e1.x
 local dy = e2.y-e1.y
 return atan2(dx, dy)
end

--cp-ed aabb, please check
check_col = function(a,b)
 return not(
 (a.x>b.x+b.w-1) or
 (b.x>a.x+a.w-1) or
 (a.y>b.y+b.h-1) or
 (b.y>a.y+a.h-1)
 )
end
