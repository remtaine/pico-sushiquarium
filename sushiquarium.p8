--[[
state machine
]]

function _update()
 if state=="menu" then
  update_menu()
 elseif state=="play" then
  update_game()
 end
end

function _draw()
 if state=="menu" then
		draw_menu()
	elseif state=="play" then
	 draw_game()
	end
end
