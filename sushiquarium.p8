__lua__
-- init
-- sushiquarium
-- by remtaine

function _init()
-- play vs menu
 state="menu"
-- enable mouse
--poke(0x5f2d, 1)


--setup 64x64 border
	poke(0x5f2c,3)
	border = 63
 money = 150


--debug
	debug = ""
--time
	fr = 0
	
	
-- coordinates for dots (feed)
 dx = 30
 dy = 10
 dprice = 5
 
 
--fish coordinates
 fish = entity:new({
  img = 1,
  --1 for right, -1 for left
  face = 1,
		speed = {x = 1, y = 0.5},
		
		update = function(self)
			if self.x < dx then
	 	 self.x += self.speed.x
			elseif self.x > dx then
	 	 self.x -= self.speed.x
	 	end
	 	
	 	if self.y < dy then
	 	 self.y += self.speed.y
			elseif self.y > dy then
	 	 self.y -= self.speed.y
	 	end
	 	
	 	if (self.x - 3 > border) then
	   self.x = -5
	  end
	
		 if (self.y - 3 > border) then
		  self.y = 5
		 end
	
		end,
		
		
		draw = function(self)
			spr(self.img,self.x,self.y)
 	 --circfill(self.x-1,self.y,1,11)
		end
 })
 
 fishes = {}
 --rn only color diff, but it should have other meanings
 fish_types = {yellow, green}
 for i = 1,5 do
  add(fishes, fish:new({
   x=flr(rnd(border)),
   y=flr(rnd(border)),
   img=flr(rnd(2))
  }))
 end
 
--shark coordinates


--arrow coordinates for food
 ax = 40
 ay = 8
 adir = 1
 as = 0.5
 amins = 1
 aspeedup = -1
 
--music(0,0,3)
end

-->8
--state machine

function _update()
 if state=="menu" then
  update_menu()
 elseif state=="play" then
  update_game()
 end
end

function _draw()
 if state=="menu" then
		draw_menu()
	elseif state=="play" then
	 draw_game()
	end
end
-->8
--updates and draw for game

function update_game()
	fr += 1
 mousex = stat(32)
 mousey = stat(33)
 
 --if btn(⬅️) then
 if btn(⬅️) then
  --sfx(1,0)
  debug = "speed"
 	aspeedup = true
 elseif aspeedup and money >= 5 and not btn(⬅️) then
 	--sfx(-1,2)
 	--sfx(0,3)
 	
 	money -= dprice
 	debug = "drop"
 	
 	dx = ax
 	dy = ay + 1
 	
 	as = amins
 	aspeedup = false
 end
 
 if fr >= 2520 then
 	fr = 0
 end
 
 --frame movement	
 --feed move
	dy += 0.5
 if (fr % 2 == 0) then
  --fish move
  for fish in all(fishes) do
	  fish:update()
  end
  
	elseif (fr % 3 == 0) then
  if aspeedup then
   as += 1
   --debug=as
		end
		
		--arrow border
		ax += as * adir
		if ax > border - 7 then
		 ax = border-7
			adir = -1
		elseif ax - 1 < 0 then
   ax = 0
		 adir = 1
		end
	end
 --end frame movement

end


function draw_game()
 cls()
 --units
	--feed
	print("●",dx,dy,8)
	--shark
	spr(17,30,54,2,1)
	--fish
	
	--sspr(8,0,8,8,f1x,f1y,16,16)
	--spr(1,f1x,f1y,1,1,true,false)	
 for fish in all(fishes) do
	 fish:draw()
 end
--draw border
	rect(0,9,border,border,12)
	rectfill(0,0,border,8,1)
	line(1,62,62,62,4)
--arrow
	print("♥",ax+1,ay,7)
	print("♥",ax,ay,9)
--ui
 m = tostring(money)
 while #m<4 do
  m = "0"..m
 end
	print("$"..m,44,2,7)

--debug
	print(debug,2, 2)
end

-->8
--updates and draw for everything else

function update_menu()
 if btn(⬅️) then
  state="play"
 end
end

function draw_menu()
	cls()
	--map(0,0,0,0,8,8)
	fc = 7
	print("press ⬅️",15,12,fc)
 print("to start",17,20,fc)
 --todo add instructions
 --print("instructions",8,40,0)
 print("btn - feed fish",4,34,fc)
 print("dont feed shark",4,42,fc)
 print("made by remtaine",0,55,fc)
end

