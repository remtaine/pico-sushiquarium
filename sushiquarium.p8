--[[
updates and draw for game
]]

function update_game()
	fr += 1
 mousex = stat(32)
 mousey = stat(33)
 
 if btn(⬅️) then
  --sfx(1,0)
  debug = "speed"
 	aspeedup = true
 elseif aspeedup and money >= 5 and not btn(⬅️) then
 	--sfx(-1,2)
 	--sfx(0,3)
 	
 	money -= dprice
 	debug = "drop"
 	
 	dx = ax
 	dy = ay + 1
 	
 	as = amins
 	aspeedup = false
 end
 
 if fr >= 2520 then
 	fr = 0
 end
 
 --frame movement	
 --feed move
	dy += 0.5
 if (fr % 2 == 0) then
  --fish move
  if f1x < dx then
 	 f1x += f1sx
		elseif f1x > dx then
 	 f1x -= f1sx
 	end
 	
 	if f1y < dy then
 	 f1y += f1sy
		elseif f1y > dy then
 	 f1y -= f1sy
 	end
 	
	elseif (fr % 3 == 0) then
  if aspeedup then
   as += 1
   --debug=as
		end
		
		--arrow border
		ax += as * adir
		if ax > border - 7 then
		 ax = border-7
			adir = -1
		elseif ax - 1 < 0 then
   ax = 0
		 adir = 1
		end
	end
 --end frame movement
	

	if (f1x - 3 > border) then
	 f1x = -5
	end
	
	if (f1y - 3 > border) then
	 f1y = 5
	end
	
	if (fr % 6 == 0) then
	end
end


function draw_game()
 cls()
 --units
	--feed
	print("●",dx,dy,8)
	--shark
	spr(17,30,54,2,1)
	--fish
	spr(1,f1x,f1y)
	--sspr(8,0,8,8,f1x,f1y,16,16)
	--spr(1,f1x,f1y,1,1,true,false)	
 circfill(f1x-1,f1y,1,11)

--draw border
	rect(0,9,border,border,12)
	rectfill(0,0,border,8,1)
	line(1,62,62,62,4)
--arrow
	print("♥",ax+1,ay,7)
	print("♥",ax,ay,9)
--ui
 m = tostring(money)
 while #m<4 do
  m = "0"..m
 end
	print("$"..m,44,2,7)

--debug
	print(debug,2, 2)
end

