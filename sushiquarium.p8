function _init()
-- play vs menu
 state="menu"
-- enable mouse
--poke(0x5f2d, 1)

--setup 64x64 border
	poke(0x5f2c,3)
	border = 63
 money = 150

--debug
	debug = ""
--time
	fr = 0
	
-- coordinates for dots (feed)
 dx = 30
 dy = 10
 dprice = 5
 
--fish 1 coordinates
 fcnt = 1
	f1x = 16
	f1y = 16
	f1sx = 1
	f1sy = 0.5

--shark coordinates

--arrow coordinates for food
 ax = 40
 ay = 8
 adir = 1
 as = 0.5
 amins = 1
 aspeedup = -1
 
--music(0,0,3)
end

